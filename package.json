{
  "name": "@kyve/solana",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "build:binaries": "yarn build && rimraf out && pkg package.json",
    "start": "node ./dist/src/index.js",
    "format": "prettier --write ."
  },
  "bin": "./dist/src/index.js",
  "pkg": {
    "scripts": "./dist/src/index.js",
    "assets": "./node_modules/@kyve/sdk/dist/proto/*",
    "targets": [
      "latest-linux-x64",
      "latest-macos-x64",
      "latest-win-x64"
    ],
    "outputPath": "out"
  },
  "dependencies": {
    "@kyve/core": "^1.0.8",
    "axios": "^0.27.2",
    "nanoid": "^3.3.3"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.41.3",
    "pkg": "^5.6.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "webhook-discord": "^3.7.8"
  }
}
